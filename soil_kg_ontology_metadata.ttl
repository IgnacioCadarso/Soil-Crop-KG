    vann:preferredNamespacePrefix "slo" ;
    voaf:vocabularyUsed <http://www.w3.org/2002/07/owl#>, <http://www.w3.org/2000/01/rdf-schema#>, <http://www.w3.org/2001/XMLSchema#>, <http://rdfdata.eionet.europa.eu/ramon/ontology/>, <http://semanticscience.org/resource/> .
