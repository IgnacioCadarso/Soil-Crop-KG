<https://w3id.org/soilkg/ontology/>
    a owl:Ontology ;
    dcterms:title "Soil Knowledge Graph Ontology" ;
    dcterms:creator "Ignacio Cadarso Quevedo" ;
    dcterms:created "2023-01-01"^^xsd:date ;
    dcterms:modified "2023-04-01"^^xsd:date ;
    dcterms:versionNumber "1.0" ;
    dcterms:description "An ontology for representing soil properties, crop data, land use, and land cover, including specific measures and classifications relevant to agricultural and environmental sciences. It integrates NUTS region classifications and provides a rich framework for the annotation and retrieval of soil and crop measurement data across Europe." ;
    dcterms:subject "Soil properties, Land Use, Land Cover, Crops" ;
